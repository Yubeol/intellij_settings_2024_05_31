
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int i,j;
        Scanner sc = new Scanner(System.in);

        int m = sc.nextInt();
        int n = sc.nextInt();

        for (i = 1; i <= n; i++) {
            for (j = 1; j <= m; j++) {
                System.out.println(i + " "+ j);
            }
        }
    }
}
